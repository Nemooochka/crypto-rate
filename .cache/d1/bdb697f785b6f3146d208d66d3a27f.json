Table .thead {\n  display: flex;\n  flex-direction: row;\n}\n\n.coinsTable .th {\n  cursor: pointer;\n}\n\n.coinsTable .th,\n.coinsTable .td {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 200px;\n  min-width: 200px;\n  height: 60px;\n  background-color: #fff;\n  text-align: center;\n}\n\n.coinsTable .tr {\n  display: flex;\n  flex-direction: row;\n}\n\n.sortingIcon {\n  position: relative;\n  display: inline-block;\n  width: 16px;\n}\n\n.sortingIcon:before,\n.sortingIcon:after {\n  content: ' ';\n  display: block;\n  position: absolute;\n  left: 0;\n  width: 0;\n  height: 0;\n  border-style: solid;\n  opacity: 0.3;\n}\n\n.sortingIcon:before {\n  top: 50%;\n  margin-top: -6px;\n  border-width: 0 4px 4px;\n  border-color: transparent transparent #161f36;\n}\n\n.sortingIcon:after {\n  top: 50%;\n  margin-top: 2px;\n  border-width: 4px 4px 0;\n  border-color: #161f36 transparent transparent;\n}\n\n@media (max-width: 900px) {\n  .coinInfo {\n    display: inline-flex;\n    flex-direction: column;\n    width: 100%;\n  }\n\n  .coinInfoBlock {\n    width: 100% !important;\n    padding: 14px 10px;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"0532008a4f13dea1ae05a4e55e59f456","cacheData":{"env":{}}}